			<left>0</left>
			<top>0</top>
			<width>100%</width>
			<height>100%</height>
			<aspectratio>keep</aspectratio>
			<texture>$INFO[Window(home).Property(fanart)]</texture>
		</control>
		<control type="button" id="3000">
			<left>-2000</left>
			<height>1</height>
			<width>1</width>
			<onclick>Action(close)</onclick>
		</control>
	</controls>
</window>
