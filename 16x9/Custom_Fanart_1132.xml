	<animation effect="fade" start="0" end="100" time="400">WindowClose</animation>
	<animation effect="fade" end="0" start="100" time="400">WindowOpen</animation>
	<controls>
		<control type="image">
			<left>0</left>
			<top>0</top>
			<width>100%</width>
			<height>100%</height>
			<aspectratio>scale</aspectratio>
			<texture>common/black.png</texture>
		</control>
